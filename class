#include<iostream>
using namespace std;
class demo
{
    private:
    int x,y,z;
    public:
    void input(int a,int b,int c)
    {
        x=a;
        y=b;
        z=c;
        
    }
    
    void display()
    {
        cout<<x<<" "<<y<<" "<<z<<" "<<endl;
    }
    
    
    
};

int main()
{
    demo d;
    d.input(1,2,3);
    d.display();
    return 1;
}
