#include <iostream>
using namespace std;
class cube
{
    int x;
    public:
    void get_cube(int a)
    {
        x=a;
        display();
    }
    void display()
    {
         cout<<x*x*x<<" "<<endl;
    }
    
};

int main()
{
     cube c;
     c.get_cube(5);

    return 0;
}
