#include<iostream>
using namespace std;
class demo
{
    int data;
    public:
    void input(int a)
    {
        data=a;
        
    }
    void add(demo a,demo b)
    {
        cout<<data+a.data+b.data;
    }
    
    
};
int main()
{
  demo d1,d2,d3;
  d1.input(2);
  d2.input(3);
  d3.input(4);
  d3.add(d1,d2);
    
    
}
